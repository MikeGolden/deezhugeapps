{
  "name": "deezzhugeapps",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "pnpm run i18n && next build",
    "start": "next start",
    "predev": "pnpm run generate",
    "prebuild": "pnm run generate",
    "lint": "next lint --dir src --fix",
    "generate": "graphql-codegen --config .graphqlrc.ts",
    "prepare": "husky install",
    "test": "playwright test",
    "i18n:extract": "formatjs extract 'src/checkout/src/**/*.{js,jsx,ts,tsx}' --format transifex --id-interpolation-pattern '[folder]/[name]/[sha512:contenthash:base64:6]' --out-file src/checkout/content/locales/en-US.json",
		"i18n:compile": "formatjs compile-folder --ast --format transifex src/checkout/content/locales src/checkout/content/compiled-locales",
		"i18n": "pnpm run i18n:extract && pnpm run i18n:compile"
  },
  "dependencies": {
    "@adyen/adyen-web": "^5.53.1",
    "@adyen/api-library": "^14.3.0",
    "@apollo/client": "^3.8.6",
    "@headlessui/react": "^1.7.17",
    "@saleor/auth-sdk": "^0.12.0",
    "clsx": "^2.0.0",
    "editorjs-html": "^3.4.3",
    "formik": "^2.4.5",
    "libphonenumber-js": "^1.10.48",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.288.0",
    "next": "13.5.5",
    "query-string": "^8.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-error-boundary": "^4.0.11",
    "react-intl": "^6.5.0",
    "react-spinners": "^0.13.8",
    "react-test-renderer": "^18.2.0",
    "react-toastify": "^9.1.3",
    "ts-invariant": "^0.10.3",
    "url-join": "^5.0.0",
    "urql": "^4.0.5",
    "xss": "^1.0.14",
    "yup": "^1.3.2",
    "zustand": "^4.4.3"
  },
  "devDependencies": {
    "@formatjs/cli": "^6.2.1",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@next/env": "^13.5.5",
    "@parcel/watcher": "^2.3.0",
    "@playwright/test": "^1.39.0",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/lodash-es": "^4.17.10",
    "@types/node": "^20",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/url-join": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.51.0",
    "eslint-config-next": "13.5.5",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-formatjs": "^4.11.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-playwright": "^0.17.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "graphql-tag": "^2.12.6",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "tailwindcss": "^3",
    "typescript": "^5",
    "wonka": "^6.3.4"
  },
  "resolutions": {
    "graphql": "16.8.1"
  },
  "packageManager": "pnpm@8.9.2",
  "engines": {
    "pnpm": ">=8.9.2",
    "node": ">=18"
  }
}
