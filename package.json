{
  "name": "deezzhugeapps",
  "author": "Mike Goldenberg (https://github.com/MikeGolden)",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "pnpm run i18n && pnpm run db:migrate && next build",
    "start": "next start",
    "predev": "pnpm run generate",
    "prebuild": "pnm run generate",
    "lint": "next lint --dir src --fix",
    "generate": "graphql-codegen --config .graphqlrc.ts",
    "prepare": "husky install",
    "i18n:extract": "formatjs extract 'src/checkout/src/**/*.{js,jsx,ts,tsx}' --format transifex --id-interpolation-pattern '[folder]/[name]/[sha512:contenthash:base64:6]' --out-file src/checkout/content/locales/en-US.json",
    "i18n:compile": "formatjs compile-folder --ast --format transifex src/checkout/content/locales src/checkout/content/compiled-locales",
    "i18n": "pnpm run i18n:extract && pnpm run i18n:compile",
    "clean": "rimraf .next .swc out coverage",
    "build-stats": "cross-env ANALYZE=true pnpm run build",
    "check-types": "tsc --noEmit --pretty",
    "test": "jest",
    "commit": "cz",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:migrate": "tsx ./scripts/dbMigrate.ts",
    "db:studio": "dotenv -c -- drizzle-kit studio",
    "test:e2e": "playwright test",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "http-server storybook-static --port 6006 --silent",
    "serve-storybook": "run-s storybook:*",
    "test-storybook:ci": "start-server-and-test serve-storybook http://localhost:6006 test-storybook",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@adyen/adyen-web": "^5.53.1",
    "@adyen/api-library": "^14.3.0",
    "@apollo/client": "^3.8.6",
    "@clerk/nextjs": "4.25.6",
    "@clerk/themes": "1.7.8",
    "@headlessui/react": "^1.7.17",
    "@hookform/resolvers": "3.3.2",
    "@libsql/client": "0.3.5",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-menubar": "1.0.4",
    "@radix-ui/react-select": "2.0.0",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-tabs": "1.0.4",
    "@radix-ui/react-toast": "1.1.5",
    "@saleor/auth-sdk": "^0.12.0",
    "@t3-oss/env-nextjs": "0.7.1",
    "@uploadthing/react": "5.6.2",
    "clsx": "^2.0.0",
    "drizzle-orm": "0.28.6",
    "editorjs-html": "^3.4.3",
    "formik": "^2.4.5",
    "libphonenumber-js": "^1.10.48",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.288.0",
    "micro": "10.0.1",
    "mongoose": "7.6.3",
    "next": "13.5.5",
    "next-seo": "6.1.0",
    "next-sitemap": "4.2.3",
    "query-string": "^8.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-error-boundary": "^4.0.11",
    "react-intl": "^6.5.0",
    "react-spinners": "^0.13.8",
    "react-test-renderer": "^18.2.0",
    "react-toastify": "^9.1.3",
    "sharp": "0.32.6",
    "svix": "1.13.0",
    "tailwindcss-animate": "1.0.7",
    "ts-invariant": "^0.10.3",
    "uploadthing": "5.7.1",
    "url-join": "^5.0.0",
    "urql": "^4.0.5",
    "xss": "^1.0.14",
    "yup": "^1.3.2",
    "zod": "3.22.4",
    "zustand": "^4.4.3"
  },
  "devDependencies": {
    "@commitlint/cli": "17.8.0",
    "@commitlint/config-conventional": "17.8.0",
    "@commitlint/cz-commitlint": "17.8.0",
    "@formatjs/cli": "^6.2.1",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@next/bundle-analyzer": "13.5.6",
    "@next/env": "^13.5.5",
    "@parcel/watcher": "^2.3.0",
    "@percy/cli": "1.27.3",
    "@percy/playwright": "1.0.4",
    "@playwright/test": "^1.39.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@storybook/addon-essentials": "7.5.0",
    "@storybook/addon-interactions": "7.5.0",
    "@storybook/addon-links": "7.5.0",
    "@storybook/blocks": "7.5.0",
    "@storybook/nextjs": "7.5.0",
    "@storybook/react": "7.5.0",
    "@storybook/test-runner": "0.13.0",
    "@storybook/testing-library": "0.2.2",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/jest": "29.5.6",
    "@types/lodash-es": "^4.17.10",
    "@types/node": "^20",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/url-join": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "autoprefixer": "^10.4.16",
    "commitizen": "4.3.0",
    "cross-env": "7.0.3",
    "cssnano": "6.0.1",
    "dotenv-cli": "7.3.0",
    "drizzle-kit": "0.19.13",
    "encoding": "0.1.13",
    "eslint": "^8.51.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-next": "13.5.5",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-formatjs": "^4.11.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "27.4.2",
    "eslint-plugin-jest-dom": "5.1.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-playwright": "^0.17.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-storybook": "0.6.15",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-testing-library": "6.1.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "graphql-tag": "^2.12.6",
    "http-server": "14.1.1",
    "husky": "^8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "^15.0.1",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "rimraf": "5.0.5",
    "semantic-release": "22.0.5",
    "start-server-and-test": "2.0.1",
    "storybook": "7.5.0",
    "tailwindcss": "^3",
    "tsx": "3.14.0",
    "typescript": "^5",
    "wonka": "^6.3.4"
  },
  "config":{
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "resolutions": {
    "graphql": "16.8.1"
  },
  "packageManager": "pnpm@8.9.2",
  "engines": {
    "pnpm": ">=8.9.2",
    "node": ">=18"
  }
}
